<?php
namespace SmartPHP\Example\Models\Entities;

/**
 * @Entity
 * @Table(name="smartphp_companies")
 */
class Company
{

    /**
     *
     * @var string 
     * 
     * @Column(name="ID", type="guid", length=255, nullable=false)
     * @Id
     * @GeneratedValue(strategy="UUID")
     */
    private $id;

    /**
     *
     * @var string 
     * 
     * @Column(name="Name", type="string", length=255, nullable=false)
     */
    private $name;
}