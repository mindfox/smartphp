{
	"name" : "maxmeffert/smartphp",
	"description" : "PHP backend utilities for Isomorphic SmartClient",
	"homepage" : "https://intrawarez.github.io/smartphp/",
	"authors" : [{
			"name" : "Maximilian Meffert",
			"email" : "packagist@intrawarez.de"
		}
	],
	"license" : "LGPL",
	"autoload" : {
		"psr-4" : {
			"" : [
				"src/",
				"example/src/"
			]
		}
	},
	"minimum-stability" : "dev",
	"prefer-stable" : true,
	"require-dev" : {
		"phpunit/phpunit" : "5.4.*",
		"squizlabs/php_codesniffer" : "^2.6",
		"phake/phake" : "^2.3"
	},
	"require" : {
		"php" : ">=7",
		"symfony/serializer" : "^3.1",
		"psr/http-message" : "^1.0",
		"slim/slim" : "^3.8",
		"doctrine/orm" : "^2.5",
		"php-di/php-di" : "^5.4",
		"php-di/slim-bridge": "^1.0"
	}
}
